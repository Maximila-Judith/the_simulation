generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:@localhost:3306/simulation"
}

model Result {
  id        Int     @id @default(autoincrement())
  tax_name  String
  tax_base  String
  amount    String
  rate      String
  minimum   Float
  price_add Float
  tax_price String
  code      String? 
  user      User?   @relation(fields: [userId], references: [id])
  userId    Int?
}

model User {
  id          Int     @id @default(autoincrement())
  email       String  @unique
  name        String?
  result_code String?
  results     Result[]
}
