generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:@localhost:3306/simulation"
}

model User {
  id          Int     @id @default(autoincrement())
  email       String  @unique
  name        String?
  result      Result?
  result_code String? @unique
}

model Result {
  id        Int     @id @default(autoincrement())
  tax_name  String
  tax_base  String
  amount    String
  rate      String
  minimum   Float
  price_add Float
  tax_price String
  code      String? @unique
  user      User?   @relation(fields: [code], references: [result_code])
}
